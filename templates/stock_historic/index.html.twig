{% extends 'base.html.twig' %}

{% block title %}StockHistoric index{% endblock %}

{% block body %}
    <h1>StockHistoric index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CreatedAt</th>
                <th>Stock</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for stock_historic in stock_historics %}
            <tr>
                <td>{{ stock_historic.id }}</td>
                <td>{{ stock_historic.createdAt ? stock_historic.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ stock_historic.stock }}</td>
                <td>
                    <a href="{{ path('app_stock_historic_show', {'id': stock_historic.id}) }}">show</a>
                    <a href="{{ path('app_stock_historic_edit', {'id': stock_historic.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_stock_historic_new') }}">Create new</a>
{% endblock %}
